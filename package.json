{
  "name": "tennis-scorigami",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "generate-types": "npx supabase gen types --lang=typescript --project-id anrvneirjnmuurloqoie --schema public > src/supabase/database.types.ts",
    "prepare": "husky",
    "commit": "npx git-cz",
    "db:generate": "dotenvx run -- drizzle-kit generate",
    "db:push": "dotenvx run -- drizzle-kit push",
    "db:pull": "dotenvx run -- drizzle-kit pull",
    "db:check": "dotenvx run -- drizzle-kit check",
    "db:studio": "dotenvx run -- drizzle-kit studio",
    "db:up": "dotenvx run -- drizzle-kit up",
    "db:migrate": "dotenvx run -- drizzle-kit migrate",
    "db:seed": "dotenvx run -- tsx src/db/scripts/seed.ts",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "test": "echo \"No tests configured\"",
  "eslintConfig": {
    "extends": "next/core-web-vitals"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@eslint/eslintrc": "github:eslint/eslintrc",
    "@lucide/lab": "^0.1.2",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@shadcn/ui": "^0.0.4",
    "@supabase/supabase-js": "^2.45.6",
    "@tanstack/react-query": "^5.59.16",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "d3": "^7.9.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "eslint-js": "github:eslint/js",
    "jotai": "^2.10.0",
    "lucide-react": "^0.452.0",
    "next": "^15.0.1",
    "next-swagger-doc": "^0.4.0",
    "next-themes": "^0.3.0",
    "pg": "^8.15.6",
    "react": "^18",
    "react-d3-tree": "^3.6.2",
    "react-dom": "^18",
    "react-social-icons": "^6.18.0",
    "realfavicon": "^0.4.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@dotenvx/dotenvx": "^1.43.0",
    "@next/eslint-plugin-next": "^15.0.3",
    "@types/d3": "^7.4.3",
    "@types/node": "^20",
    "@types/pg": "^8.15.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "commitlint": "^19.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
